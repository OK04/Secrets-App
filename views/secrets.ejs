<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
      <img src="https://img.icons8.com/cotton/84/000000/key--v2.png"/>
      <br>
      <br>
    <h1 style="font-family: 'Noto Sans JP', sans-serif; font-weight: 200" class="display-3">List of Secrets!</h1>
    <br>

<% usersWithSecrets.forEach(function(user){ %>
      <p style="font-family: 'Noto Sans JP', sans-serif;" class="secret-text"><%= user.secret %></p>
<% }); %>

<br>
    <hr>
<br>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Secret</a>
  </div>
</div>

<%- include('partials/footer') %>
